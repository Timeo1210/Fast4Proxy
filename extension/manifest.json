{
  "manifest_version": 3,
  "name": "Fast4Proxy",
  "version": "1.2.7",
  "description": "Your personalized proxy manager. Control website proxying for optimal browsing. Simple, secure, and efficient.",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "storage", "proxy"],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "background": {
    "service_worker": "src/background.js"
  }
}
